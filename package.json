{
  "name": "timeline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-client": "vue-cli-service serve",
    "build-client": "vue-cli-service build",
    "dev-server": "nodemon",
    "build-server": "tsc --build --clean --force src/server",
    "test-server": "env TS_NODE_PROJECT=src/server/tsconfig.json mocha -r ts-node/register src/server/**/*.ts --exit",
    "lint": "vue-cli-service lint --no-fix",
    "lint-autofix": "vue-cli-service lint"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.7",
    "body-parser": "^1.19.0",
    "direct-vuex": "^0.11.1",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-vuex": "^4.4.4",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint --no-fix",
      "git add"
    ]
  }
}
